<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="pt.ubi.di.pdm.tiagomartins.MenuAdmin">

    <Button
        android:id="@+id/startmenuaadddish"
        style="@android:style/Widget.Button.Small"
        android:layout_width="230dp"
        android:layout_height="88dp"
        android:layout_marginTop="88dp"
        android:drawableBottom="@mipmap/icons8_food"
        android:elevation="0dp"
        android:onClick="startAddDishMenu"
        android:text="Adicionar Prato"
        android:textAppearance="@style/TextAppearance.AppCompat.Title"
        app:layout_constraintHorizontal_bias="0.441"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1" />

    <Button
        android:id="@+id/startmenueditardish"
        style="@android:style/Widget.Button.Small"
        android:layout_width="230dp"
        android:layout_height="88dp"
        android:layout_marginTop="200dp"
        android:drawableBottom="@mipmap/icons8_edit"
        android:onClick="startEditPratoMenu"
        android:text="Editar Prato"
        android:textAppearance="@style/TextAppearance.AppCompat.Title"
        app:layout_constraintHorizontal_bias="0.441"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1" />


</android.support.constraint.ConstraintLayout>